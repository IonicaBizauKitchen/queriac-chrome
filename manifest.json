{
  "manifest_version": 2,
  "version": "1.0",
  "name": "Queriac",
  "description": "Queriac turns your address bar into a javascript command line.",
  "icons": {
    "128": "icons/icon-128.png",
    "48": "icons/icon-48.png",
    "16": "icons/icon-16.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [ "tabs" ],
  "update_url": "http://clients2.google.com/service/update2/crx"
}